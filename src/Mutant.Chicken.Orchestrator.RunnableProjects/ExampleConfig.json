{
  "$schema": "./runnableproject-volatile.json",
  "name": "Basic Console App",
  "defaultName": "ConsoleApp1",
  "shortName": "cons",
  "description": "A basic console application",
  "sources": [
    {
      "include": [
        "**/*"
      ],
      "exclude": [
        ".netnew.json",
        "**/[Bb]in/",
        "**/[Oo]bj/"
      ],
      "copyOnly": [
        "**/*.css"
      ],
      "source": "./",
      "target": "./"
    }
  ],
  "parameters": {
    "name": {
      "isName": true,
      "description": "The name of your project",
      "defaultValue": "ConsoleApp1",
      "requirement": "implicit",
      "type": "string",
      "isVariable": false
    },
    "repeat": {
      "description": "The number of times to repeat the message",
      "defaultValue": "1",
      "requirement": "optional",
      "type": "int",
      "isVariable": true
    },
    "Message": {
      "description": "The message to print",
      "requirement": "required",
      "type": "string",
      "isVariable": true
    },
    "USERNAME": {
      "description": "Pretend path",
      "requirement": "optional",
      "type": "string",
      "isVariable": true
    }
  },
  "config": {
    "variables": {
      "sources": {
        "environment": "env_{0}",
        "user": "usr_{0}"
      },
      "order": [ "environment", "user" ],
      "fallbackFormat": "{0}",
      "expand": true
    },
    "flags": {
      "conditionals": {
        "on": "/*conditionals_on*/",
        "off": "/*conditionals_off*/",
        "default": true
      }
    },
    "conditionals": {
      "if": "#if",
      "else": "#else",
      "elseif": "#elseif",
      "endif": "#endif",
      "evaluator": "C++",
      "wholeLine": true,
      "trim": true
    },
    "replacements": {
      "ConsoleAppTemplate": "name",
      "messageCount": "repeat"
    },
    "macros": {
      "{98048C9C-BF28-46BA-A98E-63767EE5E3A8}": {
        "type": "guid",
        "action": "new",
        "format": "B"
      },
      "RepeatOnce": {
        "type": "evaluate",
        "action": "(repeat == 1)"
      },
      "RepeatNone": {
        "type": "evaluate",
        "action": "(repeat == 0)"
      },
      "#define AUTHORING\r\n": {
        "type": "constant",
        "action": ""
      },
      "CURRENT_YEAR": {
        "type": "now",
        "action": "yyyy"
      }
    }
  }
}